{
    "name": "node-api-asset",
    "version": "1.0.0",
    "main": "index.js",
    "author": "PT Konekthing Benda Pintar",
    "license": "MIT",
    "scripts": {
        "test": "echo \"Error: no test specified\" && exit 1",
        "start": "node dist/index.js",
        "dev": "tsc-watch --onSuccess \"node ./dist/index.js\"",
        "build": "tsc",
        "knex:migrate:make": "knex --knexfile ./knexfile.ts migrate:make",
        "knex:migrate:latest": "knex --knexfile ./knexfile.ts migrate:latest",
        "knex:migrate:rollback": "knex --knexfile ./knexfile.ts migrate:rollback",
        "knex:migrate:up": "knex --knexfile ./knexfile.ts migrate:up",
        "knex:migrate:down": "knex --knexfile ./knexfile.ts migrate:down",
        "knex:seed:make": "knex --knexfile ./knexfile.ts seed:make",
        "knex:seed:run": "knex --knexfile ./knexfile.ts seed:run"
    },
    "devDependencies": {
        "@types/bcrypt": "^5.0.0",
        "@types/compression": "^1.7.2",
        "@types/cors": "^2.8.12",
        "@types/express": "^4.17.13",
        "@types/jsonwebtoken": "^8.5.9",
        "@types/knex": "^0.16.1",
        "@types/morgan": "^1.9.3",
        "@types/multer": "^1.4.7",
        "@types/node": "^18.7.14",
        "@types/swagger-ui-express": "^4.1.3",
        "@types/uuid": "^8.3.4",
        "@typescript-eslint/eslint-plugin": "^5.36.1",
        "@typescript-eslint/parser": "^5.36.1",
        "eslint": "^8.23.0",
        "eslint-config-prettier": "^8.5.0",
        "eslint-plugin-prettier": "^4.2.1",
        "prettier": "^2.7.1",
        "ts-node": "^10.9.1",
        "tsc-watch": "^5.0.3",
        "typescript": "^4.8.2"
    },
    "dependencies": {
        "axios": "^1.4.0",
        "bcrypt": "^5.1.0",
        "compression": "^1.7.4",
        "cors": "^2.8.5",
        "dotenv": "^16.0.2",
        "express": "^4.18.1",
        "helmet": "^6.0.0",
        "joi": "^17.6.0",
        "jsonwebtoken": "^9.0.0",
        "knex": "^2.3.0",
        "module-alias": "^2.2.2",
        "morgan": "^1.10.0",
        "multer": "^1.4.5-lts.1",
        "mysql": "^2.18.1",
        "swagger-ui-express": "^4.6.3",
        "uuid": "^9.0.0"
    },
    "_moduleAliases": {
        "@/resources": "dist/resources",
        "@/models": "dist/models",
        "@/schemas": "dist/schemas",
        "@/utils": "dist/utils",
        "@/middlewares": "dist/middlewares",
        "@/helpers": "dist/helpers"
    }
}
